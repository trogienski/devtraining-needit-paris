        <script><![CDATA[var rightNow = new GlideDateTime();
var dueSoon = new GlideRecord('x_58872_needit_needit_task');
dueSoon.addQuery('due_date','<=',gs.hoursAgo(-24));
dueSoon.addQuery('due_date','>',rightNow);
dueSoon.addQuery('state','!=',3);
dueSoon.query();
while(dueSoon.next()){
	gs.info("Task due soon: " + dueSoon.number);
	gs.eventQueue('x_58872_needit.taskDueSoon',dueSoon,dueSoon.number,gs.getUserID());
}]]></script>
